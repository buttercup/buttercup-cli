{
  "name": "buttercup-cli",
  "version": "0.2.0",
  "description": "Buttercup command line application",
  "main": "source/index.js",
  "bin": {
    "bcup": "./source/index.js"
  },
  "scripts": {
    "test": "xo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perry-mitchell/buttercup-cli.git"
  },
  "keywords": [
    "buttercup",
    "password",
    "manager",
    "security",
    "credentials"
  ],
  "author": "Perry Mitchell <perry@perrymitchell.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/perry-mitchell/buttercup-cli/issues"
  },
  "homepage": "https://github.com/perry-mitchell/buttercup-cli#readme",
  "dependencies": {
    "buttercup": "~0.20.1",
    "copy-paste": "^1.3.0",
    "inquirer": "^1.1.2",
    "pw": "0.0.4",
    "read": "^1.0.7",
    "tuck": "0.0.4"
  },
  "devDependencies": {
    "xo": "^0.16.0"
  },
  "xo": {
    "esnext": true,
    "semicolon": true,
    "space": 4,
    "envs": [
      "node"
    ],
    "rules": {
      "babel/arrow-parens": "off",
      "babel/object-curly-spacing": [
        "error",
        "always"
      ],
      "babel/object-shorthand": "off",
      "no-multi-spaces": "off",
      "no-var": "off",
      "one-var": "off",
      "prefer-arrow-callback": "off",
      "prefer-const": "off",
      "quotes": [
        "error",
        "double",
        {
          "allowTemplateLiterals": true
        }
      ],
      "space-before-function-paren": "off",
      "xo/no-process-exit": "off"
    }
  }
}
